{
  "name": "linter-selective",
  "main": "./linter.js",
  "version": "0.2.0",
  "description": "Atom bindings for selective",
  "keywords": [
    "lint",
    "linter",
    "html",
    "selector",
    "rehype"
  ],
  "repository": "https://github.com/ChristianMurphy/linter-selective",
  "license": "MIT",
  "engines": {
    "atom": ">=1.7.0 <2.0.0"
  },
  "providedServices": {
    "linter": {
      "versions": {
        "2.0.0": "provideLinter"
      }
    }
  },
  "package-deps": [
    "linter"
  ],
  "dependencies": {
    "@selective/atom": "0.2.1"
  },
  "renovate": {
    "extends": [
      "config:base"
    ],
    "automerge": true,
    "major": {
      "automerge": false
    },
    "lockFileMaintenance": {
      "enabled": true
    },
    "semanticPrefix": "chore:",
    "semanticCommitScope": ""
  }
}
